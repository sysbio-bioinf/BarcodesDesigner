<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane fx:id="contentPane" hgap="30.0" vgap="10.0" xmlns="http://javafx.com/javafx/16"
          stylesheets="@../css/darkMode/pages.css"
          xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.code.controller.InitialBarcodeSetController">
    <HBox fx:id="loadInitialSetBox" spacing="5.0" GridPane.rowIndex="0">
        <RadioButton fx:id="loadInitialSetButton" mnemonicParsing="false"/>
        <Label fx:id="loadInitialSetLabel" onMouseClicked="#selectLoadInitialSetButton" wrapText="true"/>
    </HBox>
    <Button fx:id="chooseFileButton" alignment="TOP_LEFT" disable="true" minWidth="80"
            mnemonicParsing="false" text="Choose..." onAction="#openFileChooser"
            GridPane.columnIndex="1" GridPane.rowIndex="0" GridPane.valignment="TOP"/>
    <HBox fx:id="generateInitialSetBox" spacing="5.0" GridPane.rowIndex="1">
        <RadioButton fx:id="generateInitialSetButton" mnemonicParsing="false" selected="true"/>
        <Label fx:id="generateInitialSetLabel" onMouseClicked="#selectGenerateInitialSetButton" wrapText="true"/>
    </HBox>
</GridPane>

